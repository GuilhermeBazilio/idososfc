    <sql>		drop table IDOSOS_STG.fato_derrota;
		
		select id,atleta, derrota, getdate() as data_carga 				
				
		into 
			IDOSOS_STG.fato_derrota
		from (
				select *
				from 
					(
						select atleta.id, jogo.atleta,
						count(*) over(partition by jogo.atleta) as derrota
						from IDOSOS_STG.dim_atletas atleta
						inner join IDOSOS_STG.fato_historico_jogos jogo
						on atleta.atleta = jogo.atleta
						where jogo.RESULTADO = upper('D')
					) as tb
				group by id,atleta, derrota
			) as tb
		
		create index  idx_fato_derrota on IDOSOS_STG.fato_derrota  (id);</sql>
