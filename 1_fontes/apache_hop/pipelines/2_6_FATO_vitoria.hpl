    <sql>	drop table IDOSOS_STG.fato_vitoria;
		
		select id,atleta, vitoria, getdate() as data_carga 				
				
		into 
			IDOSOS_STG.fato_vitoria
		from (
				select *
				from 
					(
						select atleta.id, jogo.atleta,
						count(*) over(partition by jogo.atleta) as vitoria
						from IDOSOS_STG.dim_atletas atleta
						inner join IDOSOS_STG.fato_historico_jogos jogo
						on atleta.atleta = jogo.atleta
						where jogo.RESULTADO = upper('V')
					) as tb
				group by id,atleta, vitoria
			) as tb
		
		create index  idx_fato_vitoria on IDOSOS_STG.fato_vitoria  (id);</sql>
